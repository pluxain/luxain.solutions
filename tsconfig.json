{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components": ["src/components/"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@libs/*": ["src/libs/*"],
      "@messages/*": ["src/paraglide/*"],
      "@types": ["src/types/"],
      "@types/*": ["src/types/*"]
    }
  }
}
